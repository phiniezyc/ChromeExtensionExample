{
  "manifest_version": 2,
  "name": "Chrome Extension Example",
  "description": "Example Chrome extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Example Chrome Extension"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "background",
    "activeTab"
  ]
}